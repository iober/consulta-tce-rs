{
  "hosting": {
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ],
    "site": "painelgovtce",
    "public": "dist/pwa",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "quasar.conf.js"
    ]
  }
}
